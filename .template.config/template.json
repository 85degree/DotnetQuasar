{
    "$schema": "http://json.schemastore.org/template",
    "author": "Abdul Gaffur A Dama",
    "classifications": [ "Web", "SPA", "Vue", "Quasar" ], 
    "name": "ASP.NET Core with Quasar",
    "identity": "Apung.DotnetQuasar",         
    "groupIdentity":"Apung.DotnetQuasar",
    "shortName": "quasar",
    "tags": {
      "language": "C#",
      "type":"project"
    },
    "sources": [
        {
            "source": "./",
            "target": "./",
            "exclude": [
                "**/[Bb]in/**",
                "**/[Oo]bj/**",
                ".template.config/**/*",
                "**/*.filelist",
                "**/*.user",
                "**/*.lock.json",
                "**/*.sln",
                "**/.git/**",
                "**/*.nuspec",
                "**/node_modules",
                ".vs/**",
                "**/.vscode/**",
                "**/ClientApp/dist/**",
                "**/wwwroot/dist/**",
                "content/Directory.Build.*",
                "**/yarn.lock"
            ]
        }
    ],
    "preferNameDirectory": true,
    "symbols":{
      "copyrightName": {
        "type": "parameter",
        "defaultValue": "Abdul Gaffur A Dama",
        "replaces":"Abdul Gaffur A Dama"
      },
      "title": {
        "type": "parameter",
        "defaultValue": "DotnetQuasar",
        "replaces":"DotnetQuasar"
      }
    }
  }